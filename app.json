{
  "expo": {
    "name": "scouterApp",
    "slug": "scouterAppFE",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "description": "Join others in talking and discussing all things anime",
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.team4.ScouterApp",
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
        
      }
    },
    "androidNavigationBar":{
      "visible":"sticky-immersive"
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    }
  }
}
